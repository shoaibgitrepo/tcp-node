const net = require("net");
// const server = net.createServer();

const server = net.createServer(function(socket) {
  socket.write("socket");
  socket.pipe(socket);
});

const port = process.env.PORT || 1337;
server.listen(port, () => {
  console.log("TCP Server is running on port " + port + ".");
});

